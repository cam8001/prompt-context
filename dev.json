    "@awslabs.cdk-mcp-server/GetAwsSolutionsConstructPattern",
    "@awslabs.cdk-mcp-server/LambdaLayerDocumentationProvider",
    "@awslabs.cdk-mcp-server/SearchGenAICDKConstructs",
    "@aws-knowledge-mcp-server/aws___read_documentation",
    "@aws-knowledge-mcp-server/aws___recommend",
    "@aws-knowledge-mcp-server/aws___search_documentation"
  ],
  "resources": [
    "file:///Users/ctod/.aws/amazonq/prompts/my-coding-style.md",
    "file://.amazonq/context/**/*.md"
  ],
  "hooks": {},
  "toolsSettings": {},
  "useLegacyMcpJson": true
}
